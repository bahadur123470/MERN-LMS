{
    "compilerOptions": {
        "ignoreDeprecations": "6.0",
        "baseUrl": ".",
        "paths": {
        "@/*": ["src/*"]
        },
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "jsx": "react-jsx"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
